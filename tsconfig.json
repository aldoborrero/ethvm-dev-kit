{
    "compilerOptions": {
        "lib": ["dom", "es5", "es2015"],
        "target": "es5",
        "strict": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "typeRoots": ["node_modules/@types"],
        "noImplicitThis": false
    }
}
